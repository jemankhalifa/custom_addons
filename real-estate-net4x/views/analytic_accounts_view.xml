<odoo>
    <data>
      
        <record id="view_listing_inherit_property" model="ir.ui.view">
            <field name="name">property.form.inherit.property</field>
            <field name="model">account.analytic.account</field>
            <field name="inherit_id" ref="industry_real_estate.property_form_view"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='x_invoice_status']" position="after">
                    
                        <field name="for_sale"/>
                        <field name="for_rent"/>
                        <field name="refrence_id"/>
                        <field name="property_type"/>
                        <field name="floor"/>
                        <field name="area_size"/>
                        <field name="balcones"/>
                        <field name="beds"/>
                        <field name="bathroom"/>
                        <field name="furniture"/>
                        <field name="parking_spaces"/>
                        <field name="maintenance"/>
                        <field name="usage"/>
                        <field name="view"/>
                       
                   


                </xpath>
            </field>
        </record>
    </data>
</odoo>