<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="action_check_receive_dashboard" model="ir.actions.act_window">
        <field name="name">Check Receive Dashboard</field>
        <field name="res_model">check.receive</field>
        <field name="view_mode">graph,pivot,list</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Analyze your received checks here.
            </p>
        </field>
    </record>

    <menuitem id="menu_dashboard_root"
              name="Dashboard"
              parent="menu_checks_root"
              sequence="40"/>

    <menuitem id="menu_check_receive_dashboard"
              name="Check Receive Dashboard"
              parent="menu_dashboard_root"
              action="action_check_receive_dashboard"
              sequence="20"/>

</odoo>